<idea-plugin require-restart="true">
  <id>deaglegross.coachsharp</id>
  <name>CoachSharp</name>
  <version>1.0.5</version>
  <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
  <vendor url="https://github.com/DeagleGross/SharpCoachPlugin">deaglegross</vendor>
  <idea-version since-build="211.7628.61" until-build="212.*" />
  <depends>com.intellij.modules.rider</depends>

  <description>
<![CDATA[
<p>Useful features for C# development</p>
]]>
  </description>

  <extensions defaultExtensionNs="com.intellij">
    <applicationConfigurable groupId="language" instance="com.jetbrains.rider.plugins.sharpcoachplugin.SampleOptionsPage" id="SampleOptionsPage" />
  </extensions>

  <actions>
    <action id="MapModelsAction" class="com.jetbrains.rider.plugins.coachsharp.MapModelsAction"
            text="Map models internals" description="Map models internals"/>
  </actions>

</idea-plugin>
